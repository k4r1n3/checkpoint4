{% extends 'base.html.twig' %}

{% block title %}Administration - Ajouter un projet{% endblock %}

{% block body %}

 <div class="d-flex justify-content-center my-3">
  <h1 class="col-10 col-sm-6 bg-dark text-light text-center p-2 m-3 fw-bold">Ajouter un projet</h1>
 </div>
 <div class="justify-content-center col-10 mx-auto mt-2 py-1 fw-bold text-dark">
  {{ form_start(form) }}
  {{ form_widget(form) }}
 </div>
 <div class="d-flex justify-content-center mt-4 mb-2 p-2">
  <button type="submit" class="btn text-light fw-bold fs-5">Envoyer</button>
 </div>
 {{ form_end(form) }}
{% endblock %}
