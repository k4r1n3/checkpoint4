{% extends 'base.html.twig' %}

{% block title %}Administration - Ajouter un projet{% endblock %}

{% block body %}
 <div class="d-flex justify-content-center p-2">
  <h1 class="col-10 col-sm-6 justify-content-center text-dark border-bottom border-dark text-center p-2 m-3 fw-bold">Ajouter un projet</h1>
 </div>
 <div class="justify-content-center col-10 mx-auto mt-2 py-1 fw-bold text-dark">
  {{ form_start(form) }}
  {{ form_widget(form) }}
  <button type="submit">Envoyer</button>
  {{ form_end(form) }}
 </div>
{% endblock %}
